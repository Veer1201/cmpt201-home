#include <pthread.h>
#include <stdio.h>

void *thread_func(void *arg) {
  char *my_msg = arg;
  printf("Message is: %s\n", my_msg);
  printf("Hii\n");
  return NULL;
}

int main(void) {
  pthread_t thread;

  pthread_create(&thread, NULL, thread_func, "hello threads!");

  pthread_join(thread, NULL);
  return 0;
}
